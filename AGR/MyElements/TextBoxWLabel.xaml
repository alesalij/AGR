<UserControl x:Class="AGR.TextBoxWLabel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AGR"
             mc:Ignorable="d" Height="19" Width="Auto">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <TextBox Name="TBox" TextWrapping="Wrap" Height="19" Text="50" HorizontalAlignment="Left" VerticalAlignment="Center" Tag=""/>
        <TextBlock Name="TBlock" Grid.Column="1" Height="19" TextWrapping="Wrap" Text="TextBlock" Width="Auto" VerticalAlignment="Center" Tag=""/>
    </Grid>
</UserControl>
